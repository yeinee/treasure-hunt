<h1>Listing engage_states</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Post</th>
      <th>Question number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @engage_states.each do |engage_state| %>
      <tr>
        <td><%= engage_state.user %></td>
        <td><%= engage_state.post %></td>
        <td><%= engage_state.question_number %></td>
        <td><%= link_to 'Show', engage_state %></td>
        <td><%= link_to 'Edit', edit_engage_state_path(engage_state) %></td>
        <td><%= link_to 'Destroy', engage_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Engage state', new_engage_state_path %>
